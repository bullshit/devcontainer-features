{
    "name": "packwiz",
    "id": "packwiz",
    "version": "1.0.3",
    "description": "packwiz is a command line tool for creating Minecraft modpacks",
    "options": {
        "favorite": {
            "type": "string",
            "enum": [
                "red",
                "gold",
                "green"
            ],
            "default": "red",
            "description": "Choose your favorite color."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
